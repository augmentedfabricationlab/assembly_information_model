                    <item name="CodeInput" type_name="gh_string" type_code="10">import Rhino.Geometry as rg

x_axis, y_axis = plane_axes

x_axis = rg.Line(x_axis.PointAtStart, x_axis.PointAtEnd)
y_axis = rg.Line(y_axis.PointAtStart, y_axis.PointAtEnd)

if x_axis.From.DistanceTo(y_axis.From) &lt; 0.01:
    origin = x_axis.From
    xvec = x_axis.Direction
    yvec = y_axis.Direction
elif x_axis.To.DistanceTo(y_axis.To) &lt; 0.01:
    origin = x_axis.To
    xvec = -x_axis.Direction
    yvec = -y_axis.Direction
elif x_axis.From.DistanceTo(y_axis.To) &lt; 0.01:
    origin = x_axis.From
    xvec = x_axis.Direction
    yvec = -y_axis.Direction
elif x_axis.To.DistanceTo(y_axis.From) &lt; 0.01:
    origin = x_axis.To
    xvec = -x_axis.Direction
    yvec = y_axis.Direction

plane = rg.Plane(origin, xvec, yvec)</item>
