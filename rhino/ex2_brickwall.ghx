from assembly_information_model.assembly import Assembly

""" Procedure 
00. Safe plane above pickup plane
10. Pickup plane
20. Safe plane above pickup plane
25. (optional) Transition planes to avoid self-collision
30. Safe plane above release plane
40. Release plane
50. Safe plane above pickup plane
"""

path_planes = []

pup_full = pick_up_plane_WCS_full
pup_half = pick_up_plane_WCS_half
pickup_frame_full = Frame(pup_full.Origin, pup_full.XAxis, pup_full.YAxis)
pickup_frame_half = Frame(pup_half.Origin, pup_half.XAxis, pup_half.YAxis)

#visualize full brick at pickup location
T = Transformation.from_frame(pickup_frame_full)
e = elem_full.transformed(T)

artist = MeshArtist(e.mesh)
elem_pickup_full = artist.draw()

#visualize half brick at pickup location
T = Transformation.from_frame(pickup_frame_half)
e = elem_half.transformed(T)

artist = MeshArtist(e.mesh)
elem_pickup_half = artist.draw()



safe_level = 0.250

#stack_num = 10
#y_loc = 0
#print list(pickup_frame.point) + list(pickup_frame.quaternion)

for key, element, data in assembly.elements(data=True):
    string_message = "This is the element with the key index %i" %key
    print string_message
    #print data
    
    if data['typology'] == "full":
        element_pickup = pickup_frame_full
    else:
        element_pickup = pickup_frame_half

    _10frame = element_pickup
    _40frame = element.tool_frame
    
    _00frame = _10frame.transformed(Translation.from_vector(_10frame.zaxis*-safe_level))
    _20frame = _00frame
    
    _30frame = _40frame.transformed(Translation.from_vector(_40frame.zaxis*-safe_level))
    _50frame = _30frame
    
    element.path = [_00frame, _10frame, _20frame, _30frame, _40frame, _50frame]
    tfs = [path_planes.append(draw_frame(frame)) for frame in element.path]
    #element.robot_base_frame = robot.base_frame
    
    #store also the robot origin in the attributes of the element
    #print assembly.data
    #data.update(robot_base_frame = base_frame.to_data())

#store also the robot origin in the attributes of the assembly
assembly.network.attributes.update(robot_base_frame = base_frame.to_data())
print assembly.network.attributes['robot_base_frame']
