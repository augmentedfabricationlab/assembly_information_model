                    <item name="CodeInput" type_name="gh_string" type_code="10">import Rhino.Geometry as rg
import math
from scriptcontext import sticky as st


from compas.geometry import Frame
from compas.geometry import Transformation
from compas.geometry import Rotation
from compas.geometry import Translation
from compas.geometry import Point

from compas_ghpython import draw_frame

from assembly_information_model.assembly import Assembly

import Rhino.Geometry as rg



length, width, height = [elem_full._source.xsize, elem_full._source.ysize, elem_full._source.zsize]
#print(width, length, height)

COURSES = 25
BRICKS_PER_COURSE = 4

MORTAR_PERPENDS = 0.025


total_length = BRICKS_PER_COURSE * length + (BRICKS_PER_COURSE - 1) * MORTAR_PERPENDS
gap_even = MORTAR_PERPENDS
gap_uneven = MORTAR_PERPENDS
gap_uneven = (total_length - (BRICKS_PER_COURSE * (length)))/BRICKS_PER_COURSE


assembly = Assembly() # initialize an empty assembly class
planes = [] # just for vizualisation


for course in range(COURSES):
    dy = course * height
    half_brick_ends = course % 2 != 0
    gap = gap_even if course % 2 == 0 else gap_uneven
    dx = 0

    bricks_in_course = BRICKS_PER_COURSE + (1 if half_brick_ends else 0)
    for j in range(bricks_in_course):

        first = j == 0
        last = j == bricks_in_course - 1

        is_half_brick = (first or last) and half_brick_ends

        if is_half_brick:
            if last:
                T = Translation.from_vector([dx - width/2, 0, dy])
            else:
                T = Translation.from_vector([dx - width/2-gap/2, 0, dy])
            e = elem_half.transformed(T)
            dx += width+gap/2
        else:
            T = Translation.from_vector([dx, 0, dy])
            e = elem_full.transformed(T)
            dx += length
        
        cpt = e.frame.point
        
        #apply rotation to the bricks according to the distance of the attractor points
        min_dist = 10
        for attr_pt in attr_pts:
            dist =  attr_pt.DistanceTo(rg.Point3d(cpt.x, cpt.y, cpt.z))
            if dist&lt;min_dist:
                min_dist = dist
            #print dist
        dist = min_dist
        R = Rotation.from_axis_and_angle([0,0,1], math.radians(1/(0.3*dist*dist)), point=e.frame.point) 
        e.transform(R)
        
        planes.append(draw_frame(e.tool_frame)) #only forvisualizing in gh
        
        typology = "half" if is_half_brick else "full"
        assembly.add_element(e, course=course, typology=typology)
        dx += gap

#        if j &lt; bricks_in_course - 1:
#            assembly.add_connection(j, j+1)

if transform:
    T = Transformation.from_frame_to_frame(Frame.worldXY(), Frame(ptransf.Origin, ptransf.XAxis, ptransf.YAxis))
    assembly.transform(T)
    assembly.network.transform(T)




